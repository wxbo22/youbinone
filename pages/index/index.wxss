/**index.wxss**/

.search {
  width: 80%;
  margin: 0 auto;
  height: 70rpx;
  display: -webkit-flex;
  -webkit-justify-content: center;
  -webkit-align-items: center;
  position: relative;
}

.icon-search {
  position: absolute;
  left: 10px;
  z-index: 5;
  background-image: url("data:image/gif;base64,");
  background-repeat: no-repeat;
  background-position: 0;
  width: 20px;
  height: 20px;
}

.inputtext {
  border: solid 1px;
  width: 100%;
  border-radius: 25px;
  padding-left: 30px;
}

.banner {
  width: 100%;
}

.banner image {
  width: 100%;
  height: 240rpx;
  border: 0;
  margin: 0;
  padding: 0;
}

.cg {
  margin: 15px;
}

.cg .cgTitle {
  font-size: 16px;
  margin: 5px;
}
.cg .bwg{
width: 47%;
text-align:center;
font-size: 14px;
padding: 5px;
float:left;
}
.cg image {
  width: 100%;
  height: 100px;
}
