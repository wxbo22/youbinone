/**app.wxss**/
page,.container{
    height: 100%;
    font-size: 28rpx;
}

image{
    margin: 0;
    padding: 0;
    border: 0;
}

@font-face {
    font-family: 'iconfont';
    src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
    font-weight: normal;
    font-style: normal;
}

.iconfont {
  font-family:"iconfont" !important;
  font-size:30rpx;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-erji:before { content: "\e600"; }

.icon-fangdajing:before { content: "\e692"; }

.icon-shijian:before { content: "\e731"; }

.icon-biaoqian:before { content: "\e603"; }

.icon-yinbo:before { content: "\e631"; }

.icon-dizhi:before { content: "\e767"; }

.icon-xing:before { content: "\e619"; }

.icon-index:before { content: "\e601"; }

.icon-jiantou_liebiaoxiangyou:before { content: "\eb03"; }

.icon-jiantou_liebiaoshouqi:before { content: "\eb04"; }

.icon-jiantou_liebiaoxiangzuo:before { content: "\eb05"; }

.icon-jiantou_liebiaozhankai:before { content: "\eb06"; }

.icon-people_fill:before { content: "\e715"; }

.icon-qrcode_fill:before { content: "\e71c"; }

.icon-service_fill:before { content: "\e727"; }

.icon-video_fill:before { content: "\e73c"; }

/* 人物部分 */
.c-people-item{
    display: flex;
    margin: 0 0 20rpx 0;
}

.c-people-left-item{
  width: 200rpx;
  height: 270rpx;
  position: relative;
  margin:0 20rpx 0 0; 
}

.c-people-music{
    position: absolute;
    z-index: 20;
    width: 100rpx;
    height: 50rpx;
    background-color: rgba(0, 0, 0, 0.4);
    display: flex;
    align-items: center;
    justify-content: center;
    border-radius: 25rpx;
    bottom:25rpx;
    left: 50rpx;
}

.c-people-music i{
    font-size: 32rpx;
    color: #fff;
}

.c-people-right-item{
    flex: 1 1 auto;
}

.c-people-left-item image{
    width: 200rpx;
    height: 270rpx;
    border-radius: 10rpx;
}

.c-people-company{
    color: #999;
    margin: 0 0 20rpx 0;
}

.c-people-title{
    font-size: 32rpx;
    font-weight: bold;
}

.c-people-descript{
    margin: 10rpx 0 20rpx;
}

.c-people-bottom{
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.cpeople-bone text{
    border:1rpx solid #ccc;
    border-radius: 5rpx;
    padding: 3rpx 6rpx;
    margin: 0 8rpx 0 0;
    font-size: 22rpx;
    color: #666;
}

.cpeople-btwo{
    display: flex;
    color: #fb8800;
    height: 70rpx;
    align-items: center;
}

.people-price text{
    font-size: 36rpx;
    font-weight:bold;
}

.people-price,.people-fuhao,.people-mfst{
    margin: 0 16rpx 0 0;
}

.people-mianfei{
    margin: 0 16rpx 0 0;
    font-size: 30rpx;
}

.people-star i{
    margin: 0 5rpx 0 0;
}

.c-people-right-bottom{
    width: 160rpx;
    height: 60rpx;
    display: flex;
    background: linear-gradient(to right, #00e7f3 , #01c0d2);
    align-items: center;
    justify-content: center;
    color: #fff;
    border-radius: 30rpx;
}
